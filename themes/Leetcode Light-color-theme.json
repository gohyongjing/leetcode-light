{
	"name": "Leetcode Light",
	"displayName": "Leetcode Light",
	"description": "Leetcode's light theme",
	"version": "0.0.2",
	"publisher": "GohYongJing",
	"author": {
		"name": "Goh Yong Jing"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gohyongjing/leetcode-light.git"
	},
	"keywords": [
		"leetcode",
		"light",
		"gohyongjing",
		"goh yong jing"
	],
	"$schema": "vscode://schemas/color-theme",
	"type": "light",
	"colors": {
	},
	"tokenColors": [
		{
			"name": "namespace",
			"scope": [
				"namespace"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "class",
			"scope": [
				"class"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "enum",
			"scope": [
				"enum"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "interface",
			"scope": [
				"interface"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "struct",
			"scope": [
				"struct"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "type parameter",
			"scope": [
				"typeParameter"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "type",
			"scope": [
				"type"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "parameter",
			"scope": [
				"parameter"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "variable",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "property",
			"scope": [
				"property"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "enum member",
			"scope": [
				"enumMember"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "decorator",
			"scope": [
				"decorator"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "event",
			"scope": [
				"event"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "function",
			"scope": [
				"function"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "method",
			"scope": [
				"method"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "macro",
			"scope": [
				"macro"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "label",
			"scope": [
				"label"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "string",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#AF00DB"
			}
		},
		{
			"name": "number",
			"scope": [
				"number"
			],
			"settings": {
				"foreground": "#098658"
			}
		},
		{
			"name": "regexp",
			"scope": [
				"regexp"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "operator",
			"scope": [
				"operator"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "declarations",
			"scope": [
				"declaration"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "definitions",
			"scope": [
				"definition"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "readonly and constants",
			"scope": [
				"readonly",
				"constant"
			],
			"settings": {
				"foreground": "#098658"
			}
		},
		{
			"name": "static members",
			"scope": [
				"static"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "deprecated items",
			"scope": [
				"deprecated"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "abstract members",
			"scope": [
				"abstract"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": "italic"
			}
		},
		{
			"name": "async functions",
			"scope": [
				"async"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "modifications",
			"scope": [
				"modification"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "documentation",
			"scope": [
				"documentation"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "default library",
			"scope": [
				"defaultLibrary"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Python keywords and placeholders",
			"scope": [
				"storage.type.function.python",
				"variable.language.special.self.python",
				"storage.type.class.python",
				"constant.character.format.placeholder.other.python"
			],
			"settings": {
				"foreground": "#0000FF"
			}
		},
		{
			"name": "Python function names and decorators",
			"scope": [
				"entity.name.function.python",
				"entity.name.function.decorator.python",
				"support.function.builtin.python",
				"support.function.magic.python",
			],
			"settings": {
				"foreground": "#795E26"
			}
		},
		{
			"name": "Python function parameters",
			"scope": [
				"variable.parameter.function.language.python",
				"variable.parameter.function.language.special.self.python"
			],
			"settings": {
				"foreground": "#001080"
			}
		},
		{
			"name": "Python strings",
			"scope": "string.quoted.single.python",
			"settings": {
				"foreground": "#A31515"
			}
		},
		{
			"name": "Python classes and types",
			"scope": [
				"entity.name.type.class.python",
				"entity.other.inherited-class.python",
				"support.type.exception.python",
				"support.type.python"
			],
			"settings": {
				"foreground": "#267F99"
			}
		},
		{
			"name": "Python miscellaneous",
			"scope": [
				"constant.other.caps.python",
				"keyword.operator.unpacking.parameter.python",
				"keyword.operator.arithmetic.python"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "TypeScript variables and properties",
			"scope": [
				"variable.other.readwrite.alias.ts",
				"variable.object.property.ts",
				"variable.parameter.ts",
				"variable.other.object.ts",
				"variable.other.property.ts",
				"variable.other.readwrite.ts"
			],
			"settings": {
				"foreground": "#001080"
			}
		},
		{
			"name": "TypeScript functions",
			"scope": [
				"entity.name.function.ts",
			],
			"settings": {
				"foreground": "#795E26"
			}
		},
		{
			"name": "TypeScript strings",
			"scope": [
				"string.quoted.single.ts",
				"punctuation.definition.string.template.begin.ts",
				"punctuation.definition.string.template.end.ts",
				"string.template.ts",
				"punctuation.definition.string.begin.ts",
				"punctuation.definition.string.end.ts",
				"string.quoted.double.ts"
			],
			"settings": {
				"foreground": "#A31515"
			}
		},
		{
			"name": "TypeScript keywords",
			"scope": [
				"storage.type.ts",
				"storage.type.enum.ts",
				"storage.type.interface.ts",
				"storage.type.function.ts",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"meta.type.annotation.ts",
				"storage.modifier.async.ts",
				"keyword.operator.expression.is.ts",
				"keyword.operator.expression.typeof.ts",
				"keyword.operator.new.ts"
			],
			"settings": {
				"foreground": "#0000FF"
			}
		},
		{
			"name": "Typescript types",
			"scope": [
				"entity.name.type.enum.ts",
				"entity.name.type.interface.ts",
				"support.type.primitive.ts",
				"entity.name.type.ts"
			],
			"settings": {
				"foreground": "#267F99"
			}
		},
		{
			"name": "Typescript variables",
			"scope": [
				"variable.other.enummember.ts",
				"variable.other.constant.ts",
			],
			"settings": {
				"foreground": "#0070C1"
			}
		},
		{
			"name": "Typescript Miscellaneous",
			"scope": [
				"keyword.operator.type.annotation.ts",
				"keyword.operator.assignment.ts",
				"keyword.operator.rest.ts",
				"keyword.operator.arithmetic.ts",
				"keyword.operator.comparison.ts",
			],
			"settings": {
				"foreground": "#000000"
			}
		},
	]
}

